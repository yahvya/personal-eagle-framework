<?php

use Sabo\Cli\Controller\ControllerMaker;
use Sabo\Cli\Extension\ExtensionAdd;
use Sabo\Cli\Model\ModelMaker;
use Sabo\Cli\Project\Project\ProjectManager;
use Sabo\Cli\Project\Project\SaboInitializer;
use Sabo\Cli\SaboCliCommand;

require_once(__DIR__ . "/vendor/autoload.php");

define("ROOT",__DIR__ . "/../");

// enregistrement des commandes, importez vos propres commandes
SaboCliCommand::registerCommands([
    ControllerMaker::class,
    ModelMaker::class,
    SaboInitializer::class,
    ExtensionAdd::class,
    ProjectManager::class
]);

SaboCliCommand::execArgvCommand($argc,$argv);